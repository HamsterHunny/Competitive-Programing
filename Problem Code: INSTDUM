#include <iostream>
#include <cmath>
using namespace std;

int main() {
	// your code goes here
	int t,x,n;
	cin>>t;
	for (int i = 0; i < t; i++) {
	int h=0;
	int y=0;
	float c=1;
        cin>>n;
	    int arr[n];
	    for (int i = 0; i < n ; i++) {
	        cin>>arr[i];
	    }
	    for (int i = 0; i < n  ; i++) {
            h=h+arr[i];
        
            if ((h/c)*100 == 100.0){
                y=y+1;
                
            }
            c=c+1;
            
        }
	    std::cout << y << std::endl;
	}
	return 0;
}
