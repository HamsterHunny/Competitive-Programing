#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,x,y;
	cin>>t;
	for (int i = 0; i < t ; i++) {
	    /* code */
	    cin>>x>>y;
	    if ((x*15)>=(2*y)){
	        std::cout << "YES" << std::endl;
	    }
	    else {
	        std::cout << "NO"<< std::endl;
	    }
	}
	return 0;
}
