#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t, n;
	cin>>t;
	for (int i = 0; i < t ; i++) {
            cin >> n;
            int arr[n];
            int h = 0;
            for (int i = 0; i < n; i++)
                cin >> arr[i];

            for (int i = 0; i < n; i++)
                if (arr[i] <=60 and arr[i]>=10)
                    h+=1;
    
            cout << h << endl;
	}
	    
	return 0;
}
