#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,x,n;
	cin>>t;
	for (int i = 0; i < t ; i++) {
	    int sum=0;
	    /* code */
	    cin>>x>>n;
	    int a[n];
	    for (int i = 0; i < n; i++) 
	        cin>>a[i];
	    for (int i = 0; i < n ; i++) {
	        sum=sum+a[i];
	        
	    }
	    if ((x-sum)/(20.0-n)<=10){
	        std::cout << "MI" << std::endl;
	        
	    }
	    else 
	     std::cout << "CSK" << std::endl;

	
	
	    
	}
	return 0;
}
